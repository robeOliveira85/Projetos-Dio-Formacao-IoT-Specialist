#include<stdio.h>

int main() {
  int opcao;
  float a, b, resultado;
  
      scanf("%d", &opcao);
      
      if(opcao > 0 && opcao < 5) {
      
      scanf("%f %f", &a, &b);
      }
      
      switch(opcao) {
      case 1: 
        printf("Multiplicacao = %.2f", a * b);
        break;
        
      case 2:
      printf("Soma = %.2f", a + b);
        break;
        
      case 3:
      printf("Subtracao = %.2f", a - b);
        break;
        
      case 4:
      printf("Divisao = %.2f", a / b);
        break;
      }
      
    getchar();
    return 0;
}