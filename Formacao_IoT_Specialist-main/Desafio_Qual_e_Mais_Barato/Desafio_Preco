#include <stdio.h>

int main(void)
{
    int a, b, c;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    int temp = 0;

    if (a < c)
    {
        temp = c;
        c = a;
        a = temp;
    }

    if (a < b)
    {
        temp = b;
        b = a;
        a = temp;
    }

    if (b < c)
    {
        temp = c;
        c = b;
        b = temp;
    }

    printf("%d, %d, %d", a, b, c);
    
 getchar();
 return 0;
}